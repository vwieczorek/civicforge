# ElevenLabs API Configuration
# Copy this file to .env and add your actual credentials

# Get your API key from: https://elevenlabs.io/app/speech-synthesis
ELEVENLABS_API_KEY=your_api_key_here

# Optional: Specify a voice ID (will list available voices if not set)
ELEVENLABS_VOICE_ID=your_preferred_voice_id

# Voice settings (optional - will use script recommendations if not set)
ELEVENLABS_STABILITY=0.75
ELEVENLABS_SIMILARITY=0.85
ELEVENLABS_STYLE=0.3